program
	i:integer;
	r:real;
	j:1..13;
	b:boolean;
	ai:Array [1..5] of integer;
	ar:Array [1..5] of real;
	aj:Array [1..5] of 1..13;
begin
	b := j = r;
	b := j = i;
	b := j = j;
	b := j = ai[1];
	b := j = aj[1];
	b := j = ar[1];
	b := j < r;
	b := j < i;
	b := j < j;
	b := j < ai[1];
	b := j < aj[1];
	b := j < ar[1];
	b := j > r;
	b := j > i;
	b := j > j;
	b := j > ai[1];
	b := j > aj[1];
	b := j > ar[1];
	b := j /= r;
	b := j /= i;
	b := j /= j;
	b := j /= ai[1];
	b := j /= aj[1];
	b := j /= ar[1];
	b := j <= r;
	b := j <= i;
	b := j <= j;
	b := j <= ai[1];
	b := j <= aj[1];
	b := j <= ar[1];
	b := j >= r;
	b := j >= i;
	b := j >= j;
	b := j >= ai[1];
	b := j >= aj[1];
	b := j >= ar[1];
end.